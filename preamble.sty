\newcommand{\filled}[1]{
    \begin{center}
        \begin{tikzpicture}
            \begin{axis}
                \addplot+[fill,fill opacity=.3] coordinates 
                {#1} --cycle;
            \end{axis}
        \end{tikzpicture}\\
    \end{center}
}
\newcommand{\points}[1]{
    \begin{center}
        \begin{tikzpicture}
            \begin{axis}[nodes near coords]
                \addplot+[only marks,
                          point meta=explicit symbolic]
                 coordinates {#1};
            \end{axis}
        \end{tikzpicture}\\
    \end{center}
}
\newcommand{\pointsnl}[1]{
    \begin{center}
        \begin{tikzpicture}
            \begin{axis}
                \addplot[only marks]
                 coordinates {#1};
            \end{axis}
        \end{tikzpicture}\\
    \end{center}
}
\newcommand{\snakearrow}[2]{
    \arrow[#1, "#2" description,
           rounded corners,
           to path={ -- ([xshift=2ex]\tikztostart.east)
                     |- (Z) [middle]
                     -- (Y) [middle]\tikztonodes
                     -| ([xshift=-2ex]\tikztotarget.west)
                     -- (\tikztotarget)
                   }
              ]\\
}

